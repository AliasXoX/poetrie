<%= form_for @image, url: images_path, html: { multipart: true } do |f| %>
  <div>
    <%= f.label :image_file, "Upload Image" %>
    <%= f.file_field :image_file, required: true %>
  </div>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title, required: true %>
  </div>
  <div>
    <%= f.submit "Upload Image" %>
  </div>
<% end %>